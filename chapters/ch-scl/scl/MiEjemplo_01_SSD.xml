<?xml version="1.0" encoding="UTF-8"?>
<scl:SCL xmlns:scl="http://www.iec.ch/61850/2006/SCL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.iec.ch/61850/2006/SCL file:///D:/My%20Documents/IEC61850/SCL/xsd/SCL.xsd">
	<scl:Header id="SCL Example 1"/>
	<scl:Substation name="Substation_01" desc="Subestacion de prueba nro 1">
		<scl:VoltageLevel name="Voltage_level_500KV">
			<!-- _________________________________Barra 1______________________________________ -->
			<scl:Bay name="Bay_Barra" desc="Barra principal">
				<scl:ConnectivityNode name="Node_Barra" pathName="Substation_01/Voltage_level_500KV/Bay_Barra/Node_Barra"/>
			</scl:Bay>
			
			<!-- _________________________________Bay 1_________________________________ -->
			<scl:Bay name="Bay_01" desc="Vano donde van los interruptores y seccionadores">
				<!-- _________________________________Bay level LNs_________________________________ -->
				<scl:LNode lnClass="CILO" desc="Logica de enclavamiento para seccionador" lnInst="1"/>
				<scl:LNode lnClass="CSWI" desc="Swich controller" lnInst="1"/>
				<scl:LNode lnClass="PIOC" desc="Proteccion - Sobrecorriente instantanea"/>

				<!-- _________________________________TCTR_________________________________ -->
				<scl:ConductingEquipment name="CurrentTransformer" type="CTR" desc="Transformador de corriente">
					<scl:Terminal bayName="Bay_01" 
										substationName="Substation_01" 
										voltageLevelName="Voltage_level_500KV" 
										cNodeName="Terminal_Primario_Trafo" 
										connectivityNode="Substation_01/Voltage_level_500KV/Bay_Barra/Node_Barra" />
					<scl:SubEquipment name="CTR_FASE_A" phase="A" desc="Fase A de mi transformador de corriente">
						<scl:LNode lnClass="TCTR" ldInst="1" desc="Nodo logico TCTR: Current Transformer - Fase a"/>
					</scl:SubEquipment>
					<!--scl:SubEquipment name="CTR_FASE_B" phase="B" desc="Fase B de mi transformador de corriente">
						<scl:LNode lnClass="TCTR" ldInst="2" desc="Nodo logico TCTR: Current Transformer - Fase b"/>
					</scl:SubEquipment>
					<scl:SubEquipment name="CTR_FASE_C" phase="C" desc="Fase C de mi transformador de corriente">
						<scl:LNode lnClass="TCTR" ldInst="3" desc="Nodo logico TCTR: Current Transformer - Fase C"/>
					</scl:SubEquipment-->
				</scl:ConductingEquipment>

				<!-- _________________________________XCBR_________________________________ -->
				<scl:ConductingEquipment name="Interruptor_01" type="CTR" desc="Interruptor de corriente">
					<scl:LNode lnClass="XCBR" ldInst="1" desc="Nodo logico Interruptor: XCBR"/>
					<scl:Terminal bayName="Bay_01" 
										substationName="Substation_01" 
										voltageLevelName="Voltage_level_500KV" 
										cNodeName="Terminal_Positiva_XCBR" 
										connectivityNode="Substation_01/Voltage_level_500KV/Bay_01/NodeB"/>
					<scl:Terminal bayName="Bay_01" 
										substationName="Substation_01" 
										voltageLevelName="Voltage_level_500KV" 
										cNodeName="Terminal_Negativa_XCBR" 
										connectivityNode="Substation_01/Voltage_level_500KV/Bay_01/Node_C"/>
				</scl:ConductingEquipment>

				<!-- _________________________________XSWI_________________________________-->
				<scl:ConductingEquipment name="DisconnectingSwitch_01" type="DIS" desc="Seccionador 1">
					<scl:LNode lnClass="XSWI" lnInst="1" desc="Nodo logico Seccionador 1"/>
					<scl:Terminal bayName="Bay_01" 
										substationName="Substation_01" 
										voltageLevelName="Voltage_level_500KV" 
										cNodeName="Terminal_Positiva_XSWI_01" 
										connectivityNode="Substation_01/Voltage_level_500KV/Bay_01/Node_A"/>
					<scl:Terminal bayName="Bay_01" 
										substationName="Substation_01" 
										voltageLevelName="Voltage_level_500KV" 
										cNodeName="Terminal_Negativa_XSWI_01" 
										connectivityNode="Substation_01/Voltage_level_500KV/Bay_01/Node_B"/>
				</scl:ConductingEquipment>

				<scl:ConductingEquipment name="DisconnectingSwitch_02" type="DIS" desc="Seccionador 2">
					<scl:LNode lnClass="XSWI" lnInst="2" desc="Nodo logico Seccionador 2"/>
					<scl:Terminal bayName="Bay_01" 
										substationName="Substation_01" 
										voltageLevelName="Voltage_level_500KV" 
										cNodeName="Terminal_Positiva_XSWI_02" 
										connectivityNode="Substation_01/Voltage_level_500KV/Bay_01/Node_C"/>
					<scl:Terminal bayName="Bay_01" 
										substationName="Substation_01" 
										voltageLevelName="Voltage_level_500KV" 
										cNodeName="Terminal_Negativa_XSWI_02" 
										connectivityNode="Substation_01/Voltage_level_500KV/Bay_01/Node_D"/>
				</scl:ConductingEquipment>

				<!-- _________________________________Kirchoff's Nodes _________________________________-->
				<scl:ConnectivityNode name="Node_A" 
												 pathName="Substation_01/Voltage_level_500KV/Bay_01/Node_A" 
												 desc="Nodo A"/>

				<scl:ConnectivityNode name="Node_B" 
												 pathName="Substation_01/Voltage_level_500KV/Bay_01/Node_B" 
												 desc="Nodo B"/>

				<scl:ConnectivityNode name="Node_C" 
												 pathName="Substation_01/Voltage_level_500KV/Bay_01/Node_C" 
												 desc="Nodo C"/>
												 
				<scl:ConnectivityNode name="Node_D" 
												 pathName="Substation_01/Voltage_level_500KV/Bay_01/Node_D" 
												 desc="Nodo D"/>
												 
			</scl:Bay>
		</scl:VoltageLevel>
	</scl:Substation>
